#! /usr/bin/vvp
:ivl_version "10.2 (stable)" "(v10_2)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x555df0978130 .scope module, "testbench" "testbench" 2 5;
 .timescale -9 -9;
v0x555df09d77c0_0 .var "c", 2 0;
v0x555df09d78a0_0 .net "data_out", 7 0, v0x555df09d74c0_0;  1 drivers
v0x555df09d7970_0 .var "src_a", 7 0;
v0x555df09d7a70_0 .var "src_b", 7 0;
S_0x555df09782b0 .scope module, "ALU_instance" "alu" 2 10, 3 3 0, S_0x555df0978130;
 .timescale -9 -9;
    .port_info 0 /INPUT 8 "src_a"
    .port_info 1 /INPUT 8 "src_b"
    .port_info 2 /INPUT 3 "c"
    .port_info 3 /OUTPUT 8 "data_out"
v0x555df09b0630_0 .net "c", 2 0, v0x555df09d77c0_0;  1 drivers
v0x555df09d74c0_0 .var "data_out", 7 0;
v0x555df09d75a0_0 .net "src_a", 7 0, v0x555df09d7970_0;  1 drivers
v0x555df09d7660_0 .net "src_b", 7 0, v0x555df09d7a70_0;  1 drivers
E_0x555df09b1580 .event edge, v0x555df09d7660_0, v0x555df09d75a0_0, v0x555df09b0630_0;
    .scope S_0x555df09782b0;
T_0 ;
    %wait E_0x555df09b1580;
    %load/vec4 v0x555df09b0630_0;
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 3;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 3;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 5, 0, 3;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %load/vec4 v0x555df09d75a0_0;
    %store/vec4 v0x555df09d74c0_0, 0, 8;
    %jmp T_0.6;
T_0.0 ;
    %load/vec4 v0x555df09d75a0_0;
    %load/vec4 v0x555df09d7660_0;
    %add;
    %store/vec4 v0x555df09d74c0_0, 0, 8;
    %jmp T_0.6;
T_0.1 ;
    %load/vec4 v0x555df09d75a0_0;
    %load/vec4 v0x555df09d7660_0;
    %sub;
    %store/vec4 v0x555df09d74c0_0, 0, 8;
    %jmp T_0.6;
T_0.2 ;
    %load/vec4 v0x555df09d75a0_0;
    %load/vec4 v0x555df09d7660_0;
    %and;
    %store/vec4 v0x555df09d74c0_0, 0, 8;
    %jmp T_0.6;
T_0.3 ;
    %load/vec4 v0x555df09d75a0_0;
    %load/vec4 v0x555df09d7660_0;
    %or;
    %store/vec4 v0x555df09d74c0_0, 0, 8;
    %jmp T_0.6;
T_0.4 ;
    %load/vec4 v0x555df09d75a0_0;
    %load/vec4 v0x555df09d7660_0;
    %xor;
    %store/vec4 v0x555df09d74c0_0, 0, 8;
    %jmp T_0.6;
T_0.6 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x555df0978130;
T_1 ;
    %vpi_call 2 15 "$dumpfile", "simpleALU.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars" {0 0 0};
    %pushi/vec4 85, 0, 8;
    %store/vec4 v0x555df09d7970_0, 0, 8;
    %pushi/vec4 26, 0, 8;
    %store/vec4 v0x555df09d7a70_0, 0, 8;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x555df09d77c0_0, 0, 3;
    %delay 50, 0;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x555df09d77c0_0, 0, 3;
    %delay 50, 0;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x555df09d77c0_0, 0, 3;
    %delay 50, 0;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x555df09d77c0_0, 0, 3;
    %delay 50, 0;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x555df09d77c0_0, 0, 3;
    %delay 50, 0;
    %pushi/vec4 5, 0, 3;
    %store/vec4 v0x555df09d77c0_0, 0, 3;
    %delay 50, 0;
    %pushi/vec4 6, 0, 3;
    %store/vec4 v0x555df09d77c0_0, 0, 3;
    %delay 50, 0;
    %pushi/vec4 7, 0, 3;
    %store/vec4 v0x555df09d77c0_0, 0, 3;
    %end;
    .thread T_1;
    .scope S_0x555df0978130;
T_2 ;
    %delay 50, 0;
    %vpi_call 2 39 "$monitor", "time: %8d, a:%d, b:%d, c:%d, d_out:%d", $time, v0x555df09d7970_0, v0x555df09d7a70_0, v0x555df09d77c0_0, v0x555df09d78a0_0 {0 0 0};
    %jmp T_2;
    .thread T_2;
    .scope S_0x555df0978130;
T_3 ;
    %delay 400, 0;
    %vpi_call 2 42 "$finish" {0 0 0};
    %end;
    .thread T_3;
